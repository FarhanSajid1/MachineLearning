        "x = layers.Dropout(.5)(x)\n",
        "age_prediction = layers.Dense(4, activation = 'softmax', name='age')(x)\n",
        "\n",
        "model = Model(inputs, [gender_prediction, age_prediction])\n",
        "\n",
        "model.compile(optimizer= optimizers.RMSprop(lr=2e-5),\n",
