        "x = layers.Dropout(.5)(x)\n",
        "x = layers.Dense(156, activation='relu')(x)\n",
        "gender_prediction = layers.Dense(1, activation = 'sigmoid', name='gender')(x)\n",
        "\n",
        "\n",
        "x = conv_base(inputs)\n",
